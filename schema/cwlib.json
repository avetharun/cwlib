{
	"definitions": {},
	"$schema": "http://json-schema.org/draft-07/schema#", 
	"$id": "https://raw.githubusercontent.com/avetharun/cwlib/main/schema/cwlib.json", 
	"title": "CWlib schema for bare JSON objects", 
	"type": "object",
	"required": [
		"layout"
	],
	"properties": {
		"layout": {
			"$id": "#root/layout", 
			"title": "Layout", 
			"type": "array",
			"default": [],
			"items":{
				"$id": "#root/layout/items", 
				"title": "Items", 
				"type": "object",
				"required": [
					"id",
					"schema",
					"asset_data"
				],
				"properties": {
					"id": {
						"$id": "#root/layout/items/id", 
						"title": "Id", 
						"type": "string",
						"default": "",
						"examples": [
							""
						],
						"pattern": "^.*$"
					},
					"schema": {
						"$id": "#root/layout/items/schema", 
						"title": "Schema", 
						"type": "array",
						"default": []
					},
					"asset_data": {
						"$id": "#root/layout/items/asset_data", 
						"title": "Asset_data", 
						"type": "object",
						"required": [
						],
						"properties": {
						}
					}

				}
			}

		}
	}
}
